{
    "name": "lexasoft/phpci-telegram-plugin",
    "description": "PHPCI plugin for Telegram",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "LEXASOFT",
            "email": "lexasoft83@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "classmap": [ 
            "src/Telegram.php"
        ]
    },
    "extra": {
        "phpci": {
            "pluginNamespace": "PHPCI\\Plugin\\",
            "suppliedPlugins": [
                {
                    "name": "Telegram",
                    "class": "Telegram"
                }
            ]
        }
    }
}
